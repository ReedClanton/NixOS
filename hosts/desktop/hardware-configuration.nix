  # networking.interfaces.wlp2s0u1u2.useDHCP = lib.mkDefault true;
  # networking.interfaces.wlp4s6.useDHCP = lib.mkDefault true;

  nixpkgs.hostPlatform = lib.mkDefault "x86_64-linux";
